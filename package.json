  "homepage": "https://acl.com",
  "config": {
    "theme": "default",
    "coreui_library_short_version": "5.3",
    "coreui_library_docs_url": "https://acl.com/docs/"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4203",
    "start-backoffice": "ng serve backoffice --port 4205",
    "start-with-proxy": "ng serve --port 4203 --proxy-config proxy.conf.json",
    "dev": "COPY .\\src\\environments\\environment.development.ts .\\src\\environments\\environment.ts &&  npm run start-with-proxy",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prod-env": "COPY .\\src\\environments\\environment.prod.ts .\\src\\environments\\environment.ts",
    "deploy-prod": "npm run prod-env && docker-compose -f prod-docker-compose.yml up -d --build",
    "generate-icons": "svg-generator",
    "make:hello": "make HelloWorld",
    "make:goodbye": "make Goodbyeworld",
    "make:update": "make UpdateSoftware",
    "make:run-update": "make RunUpdate",
    "prepare": "husky install"
  },
  "svgGenerator": {
    "outputPath": "./src/app/svg",
    "prefix": "dfret",
    "srcPath": "./src/assets/svg",
    "svgoConfig": {
      "plugins": [
        "removeDimensions",
        "cleanupAttrs"
      ]
    }
  },
  "private": true,
